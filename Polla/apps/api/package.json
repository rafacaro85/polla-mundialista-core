  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "seed:prod": "node dist/database/seeds/wc2026-real.seeder.js",
    "seed:ucl-beta": "ts-node -r tsconfig-paths/register src/database/seeds/seed-ucl-beta.ts",
    "seed:champions:prod": "ts-node -r tsconfig-paths/register src/database/seeds/seed-ucl-beta.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:matches": "ts-node -r tsconfig-paths/register src/database/seeds/match.seeder.ts",
    "seed:ranking": "ts-node -r tsconfig-paths/register src/database/seeds/ranking.seeder.ts",
    "seed:reset-matches": "ts-node -r tsconfig-paths/register src/database/seeds/reset-matches.seeder.ts",
    "seed:promote-admin": "ts-node -r tsconfig-paths/register src/database/seeds/promote-admin.seeder.ts",
    "seed:list-users": "ts-node -r tsconfig-paths/register src/database/seeds/list-users.seeder.ts",
    "promote:me": "ts-node -r tsconfig-paths/register src/database/seeds/promote-me.seeder.ts",
    "seed:ai": "ts-node -r tsconfig-paths/register src/scripts/seed-ai-predictions.ts",
    "migrate:manual-lock": "ts-node -r tsconfig-paths/register src/database/migrations/add-manual-lock-columns.ts",
    "promote:racv": "ts-node -r tsconfig-paths/register src/database/seeds/promote-racv.seeder.ts",
    "debug:users": "ts-node -r tsconfig-paths/register src/database/seeds/debug-users.seeder.ts",
    "seed:qatar-full": "ts-node -r tsconfig-paths/register src/database/seeds/qatar-full.seeder.ts",
    "seed:bracket-playable": "ts-node -r tsconfig-paths/register src/database/seeds/bracket-playable.seeder.ts",
    "seed:complete": "ts-node -r tsconfig-paths/register src/database/seeds/complete-tournament.seeder.ts",
    "seed:wc2026": "ts-node -r tsconfig-paths/register src/database/seeds/world-cup-2026-matches.seeder.ts",
    "seed:wc2026-real": "ts-node -r tsconfig-paths/register src/database/seeds/wc2026-real.seeder.ts",
    "seed:shells": "ts-node -r tsconfig-paths/register src/database/seeds/shells.seeder.ts",
    "seed:reset-knockout": "npx ts-node src/reset-knockout-full.ts",
    "seed:ucl": "ts-node -r tsconfig-paths/register ../../scripts/seed-ucl.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts"
  },
